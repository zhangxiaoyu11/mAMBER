#!/bin/csh -f

../../../bin/parmchk2 -i ccl3br.prepi -f prepi -o ccl3br.frcmod || goto error
../../../bin/parmchk2 -i diethylamine.prepi -f prepi -o diethylamine.frcmod || goto error
../../../bin/parmchk2 -i N-methylacetamide.prepi -f prepi -o N-methylacetamide.frcmod || goto error

../../dacdif ccl3br.frcmod.save ccl3br.frcmod
../../dacdif diethylamine.frcmod.save diethylamine.frcmod
../../dacdif N-methylacetamide.frcmod.save N-methylacetamide.frcmod

exit(0)

error:
echo "  ${0}:  Program error"
exit(1)
